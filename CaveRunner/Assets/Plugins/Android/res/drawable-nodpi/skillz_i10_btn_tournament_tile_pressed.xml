<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item>
	    <layer-list>
	        <item>
	            <shape android:shape="rectangle">
	                <gradient android:startColor="#a9a9a9"
	                    android:endColor="#464646"
	                    android:angle="270"/>
	                <corners android:radius="@dimen/skillz_i10_btn_corner_radius"/>
	                <stroke android:width="@dimen/skillz_i10_btn_outer_stroke"
	            		android:color="@color/skillz_i10_btn_outer_stroke"/>
	            </shape>
	        </item>
	        <item>
	            <bitmap android:src="@drawable/skillz_i10_btn_bg_logo"
	                android:gravity="center"/>
	        </item>
	        <item android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
	            <shape android:shape="rectangle">
	                <gradient android:startColor="#dd7d8089"
	                    android:endColor="#dd5e5f64"
	                    android:angle="270" />
	                <corners android:radius="@dimen/skillz_i10_btn_corner_radius"/>
	            </shape>
	        </item>
	    </layer-list>
	</item>

</layer-list>