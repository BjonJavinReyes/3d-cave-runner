<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item>
         <shape android:shape="rectangle">
             <gradient android:startColor="#bbbdba"
                 android:centerColor="#2e302d"
                 android:endColor="#404143"
                 android:angle="270" />
             <corners android:radius="@dimen/skillz_i10_btn_corner_radius"/>
             <stroke android:width="@dimen/skillz_i10_btn_outer_stroke"
         		android:color="@color/skillz_i10_btn_outer_stroke"/>
         </shape>
     </item>
     <item android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
         <shape android:shape="rectangle">
             <gradient android:startColor="#606060"
                 android:endColor="#363636"
                 android:angle="270" />
             <corners android:radius="@dimen/skillz_i10_btn_corner_radius"/>
         </shape>
     </item>
</layer-list>