    <style name="Skillz.I10.Text.Blue">
        <item name="android:textColor">@color/skillz_i10_text_blue</item>
    </style>
    <style name="Skillz.I10.Text.Blue.Micro">
        <item name="android:textSize">@dimen/skillz_i10_text_size_micro</item>
    </style>
    <style name="Skillz.I10.Text.Blue.Micro.Bold">
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    <style name="Skillz.I10.Text.Blue.Small">
        <item name="android:textSize">@dimen/skillz_i10_text_size_small</item>
