        <item name="android:shadowRadius">0.0001</item>
    </style>
    <style name="Skillz.I10.Text.White.Medium">
        <item name="android:textSize">@dimen/skillz_i10_text_size_medium</item>
    </style>
    <style name="Skillz.I10.Text.White.Medium.Bold">
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    
    <style name="Skillz.I10.Text.Gray">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
