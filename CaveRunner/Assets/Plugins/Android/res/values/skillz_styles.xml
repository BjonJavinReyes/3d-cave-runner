        <item name="android:background">@drawable/skillz_i10_bg_spinner</item>
    </style>
    
    <style name="Skillz.I10.ActionBar">
        
    </style>
    <style name="Skillz.I10.ActionBar.Text">
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    <style name="Skillz.I10.ActionBar.Text">
        <item name="android:textSize">@dimen/skillz_i10_text_size_small</item>
    </style>
    <style name="Skillz.I10.ActionBar.Text.Gray">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
    </style>
    <style name="Skillz.I10.ActionBar.Text.Blue">
        <item name="android:textColor">@color/skillz_i10_text_blue</item>
    </style>
    <style name="Skillz.I10.ActionBar.Menu">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_micro</item>
    </style>
    
    <style name="Skillz.I10.Text">
        <item name="android:contentDescription">@string/skillz_font_avenir_roman</item>
    </style>
    <style name="Skillz.I10.Text.White">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
    </style>
    <style name="Skillz.I10.Text.White.Micro">
        <item name="android:textSize">@dimen/skillz_i10_text_size_micro</item>
    </style>
    <style name="Skillz.I10.Text.White.Micro.Bold">
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    <style name="Skillz.I10.Text.White.Small">
        <item name="android:textSize">@dimen/skillz_i10_text_size_small</item>
    </style>
    <style name="Skillz.I10.Text.White.Small.Bold">
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    <style name="Skillz.I10.Text.White.Small.Bold.Shadow">
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
        <item name="android:shadowDx">0</item>
