<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="android:Theme.Light" />

    <style name="Skillz">
    </style>
    
    <style name="SkillzDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- Headers -->
    
    <style name="Skillz.H1">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">22sp</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">0.0001</item> 
    </style>
    
    <style name="Skillz.H2">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    
    <style name="Skillz.H3">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">0.0001</item> 
    </style>
    
    <style name="Skillz.H4">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">-1</item>
		<item name="android:shadowRadius">0.0001</item> 
    </style>
    
    <style name="Skillz.H5">
        
    </style>
    
    <!-- Inputs -->
    
    <style name="Skillz.Input">
        
    </style>
    
    <style name="Skillz.Input.Btn">
        <item name="android:gravity">center_horizontal</item>
    </style>
    
    <style name="Skillz.Input.Btn.Action">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_height">36dip</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/skillz_text_white</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">-1</item>
        <item name="android:shadowRadius">0.0001</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">10dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    
    <style name="Skillz.Input.Btn.Card">
		<item name="android:background">@android:color/transparent</item>
        <item name="android:layout_height">@dimen/skillz_button_height</item>
		<item name="android:textSize">@dimen/skillz_button_text_size</item>
		<item name="android:textColor">#FFFFFF</item>
		<item name="android:textStyle">bold</item>
		<item name="android:contentDescription">@string/skillz_font_avenir_black</item>
		<item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">-1</item>
		<item name="android:shadowRadius">0.0001</item>  
		<item name="android:gravity">center</item> 
		<item name="android:singleLine">true</item>
    </style>
    
    <style name="Skillz.Input.Btn.Card.Small">
        <item name="android:textSize">18sp</item>
    </style>
    
    <style name="Skillz.Input.Btn.Card.Tournament">
        <item name="android:textSize">22sp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    
    <style name="Skillz.Input.Text">
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/skillz_bg_text_field</item>
    </style>
    
    <style name="Skillz.Input.Text.Small">
        <item name="android:textSize">16sp</item>
    </style>
    
    <style name="Skillz.Input.Text.Small.SingleLine">
        <item name="android:singleLine">true</item>
    </style>
    
    <style name="Skillz.Input.CheckBox">
        <item name="android:button">@drawable/skillz_checkbox_button</item>
        <item name="android:background">@drawable/skillz_bg_text_field</item>
        <item name="android:layout_height">40dip</item>
        <item name="android:layout_width">40dip</item>
    </style>
        
    <style name="Skillz.Input.Text.SingleLine">
        <item name="android:singleLine">true</item>
    </style>
    
    <style name="Skillz.Input.Text.SingleLine.Inverse">
        <item name="android:background">@drawable/skillz_bg_info_text_field</item>
        <item name="android:textColor">@color/skillz_text_white</item>
    </style>
    
    <style name="Skillz.Input.Text.SingleLine.Inverse.Bold">
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="Skillz.Input.Text.SingleLine.Inverse.Green">
        <item name="android:textColor">@color/skillz_text_green</item>
    </style>
    
    <!-- Paragraph -->
    
    <style name="Skillz.P">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">normal</item>
    </style>
    
    <style name="Skillz.P.Bold">
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="Skillz.P.Green">
        <item name="android:textColor">@color/skillz_text_green</item>
    </style>
    
    <style name="Skillz.P.Large">
		<item name="android:textSize">20sp</item>        
    </style>
    
    <style name="Skillz.P.Medium">
		<item name="android:textSize">18sp</item>        
    </style>
    
    <style name="Skillz.P.Small">
        <item name="android:textSize">12sp</item>
    </style>
    
    <style name="Skillz.P.Small.Green">
        <item name="android:textColor">@color/skillz_text_green</item>
    </style>
    
    <style name="Skillz.P.Small.Bold">
        <item name="android:textStyle">bold</item>
    </style>
    
    <!-- Divs -->
    
    <style name="Skillz.View">
        <item name="android:background">@android:color/transparent</item>
    </style>
    
    <style name="Skillz.View.Card">
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">@dimen/skillz_card_margin_top</item>
        <item name="android:layout_marginBottom">0dp</item>
    </style>

    <style name="Skillz.View.CardStacked">
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
    </style>
    
    <style name="Skillz.View.CardHeader">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
		<item name="android:contentDescription">@string/skillz_font_avenir_black</item>
		<item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">-1</item>
		<item name="android:shadowRadius">0.0001</item>
		<item name="android:gravity">center</item>     
    </style>
    
    <style name="Skillz.View.ListView">
        <item name="android:dividerHeight">0dp</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:listSelector">@drawable/skillz_list_selector</item>
    </style>
    
    <style name="Skillz.View.Pill">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">46dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">-1</item>
		<item name="android:shadowRadius">0.0001</item>
    </style>
    
    <!-- Table styles -->
    
    <style name="Skillz.Table">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/skillz_text_white</item>
        <item name="android:layout_marginLeft">10dip</item>
    </style>
    
    <style name="Skillz.Table.Header">
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    
    <!-- Iteration 20 -->
    
    <style name="Skillz.I20">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="Skillz.I20.CardText">
        <item name="android:contentDescription">@string/skillz_font_helvetica_thin</item>
        <item name="android:textSize">@dimen/skillz_card_section_text_size</item>
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
    </style>
    
    <style name="Skillz.I20.MoreInfoBtnSmall">
        <item name="android:singleLine">false</item>
        <item name="android:contentDescription">@string/skillz_font_helvetica_roman</item>
        <item name="android:textSize">@dimen/skillz_more_info_small_text</item>
        <item name="android:textColor">@color/skillz_i10_text_white</item>
    </style>
    
    <style name="Skillz.I20.MoreInfoBtnLarge">
        <item name="android:contentDescription">@string/skillz_font_helvetica_black_cn</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_medium</item>
        <item name="android:textColor">@color/skillz_i10_text_white</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">none</item>
    </style>
    
    <style name="Skillz.I20.MoreInfoBtnArrow">
        <item name="android:layout_width">@dimen/skillz_more_info_ic_arrow_width</item>
        <item name="android:layout_height">@dimen/skillz_more_info_ic_arrow_height</item>
        <item name="android:layout_marginLeft">@dimen/skillz_more_info_ic_arrow_margin</item>
        <item name="android:layout_marginRight">@dimen/skillz_more_info_ic_arrow_margin</item>
    </style>
    
    <style name="Skillz.I20.MoreInfoSmall">
        <item name="android:contentDescription">@string/skillz_font_helvetica_roman</item>
        <item name="android:textSize">@dimen/skillz_more_info_small_text</item>
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:paddingLeft">@dimen/skillz_more_info_bg_padding_left</item>
    </style>
    
    <style name="Skillz.I20.MoreInfoJoin">
        <item name="android:contentDescription">@string/skillz_font_helvetica_black_cn</item>
        <item name="android:textSize">@dimen/skillz_more_info_join_text</item>
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:layout_width">@dimen/skillz_more_info_join_width</item>
        <item name="android:gravity">center</item>
    </style>
    
    
    <style name="Skillz.I20.Input">
        <item name="android:textColor">#b9b9b9</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_medium</item>
    </style>
    <style name="Skillz.I20.Input.Text">
        <item name="android:layout_height">@dimen/skillz_text_field_height</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/skillz_i20_bg_text_field_default</item>
        <item name="android:paddingLeft">@dimen/skillz_text_field_padding_left</item>
        <item name="android:paddingRight">@dimen/skillz_text_field_padding_right</item>
    </style>
    <style name="Skillz.I20.Input.Text.Margin">
        <item name="android:layout_margin">@dimen/skillz_text_field_margin</item>
    </style>
    
    <!-- Iteration 10 -->
    <style name="Skillz.I10">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="Skillz.I10.H0">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_extra_large</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.0001</item>
    </style>
    <style name="Skillz.I10.H0.Gray">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:shadowDy">-1</item>
    </style>
    <style name="Skillz.I10.H0.Blue">
        <item name="android:textColor">@color/skillz_i10_text_blue</item>
    </style>
    
    <style name="Skillz.I10.H1">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_large</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.0001</item>
    </style>
    <style name="Skillz.I10.H1.Gray">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_light</item>
        <item name="android:shadowDy">-1</item>
    </style>
    <style name="Skillz.I10.H1.Blue">
        <item name="android:textColor">@color/skillz_i10_text_blue</item>
    </style>
    <style name="Skillz.I10.H1.White">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
    </style>
    
    <style name="Skillz.I10.H2">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_medium</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.0001</item>
    </style>
    <style name="Skillz.I10.H2.Gray">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_light</item>
        <item name="android:shadowDy">-1</item>
    </style>
    <style name="Skillz.I10.H2.Blue">
        <item name="android:textColor">@color/skillz_i10_text_blue</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
    </style>
    <style name="Skillz.I10.H2.White">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
        <item name="android:shadowDy">1</item>
    </style>
    
    <style name="Skillz.I10.H3">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_small</item>
        <item name="android:contentDescription">@string/skillz_font_avenir_black</item>
    </style>
    <style name="Skillz.I10.H3.Gray">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_light</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">-1</item>
        <item name="android:shadowRadius">0.0001</item>
    </style>
    <style name="Skillz.I10.H3.White">
        <item name="android:textColor">@color/skillz_i10_text_white</item>
        <item name="android:shadowColor">@color/skillz_i10_shadow_dark</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.0001</item>
    </style>
    
    <style name="Skillz.I10.Input">
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
        <item name="android:textSize">@dimen/skillz_i10_text_size_medium</item>
        <item name="android:contentDescription">@string/skillz_font_helvetica_roman</item>
    </style>
    
    <style name="Skillz.I10.Input.Text">
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/skillz_i10_bg_text_field</item>
    </style>
    <style name="Skillz.I10.Input.Text.Dark">
        <item name="android:background">@drawable/skillz_i10_bg_text_field_dark</item>
    	<item name="android:textColor">@color/skillz_i10_text_field_dark</item>
    </style>
    
    <style name="Skillz.I10.Input.Text.Multiline">
        <item name="android:singleLine">false</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:gravity">top</item>
    </style>
    <style name="Skillz.I10.Input.Text.Multiline.Dark">
        <item name="android:background">@drawable/skillz_i10_bg_text_field_dark</item>
        <item name="android:textColor">@color/skillz_i10_text_field_dark</item>
    </style>
    
    <style name="Skillz.I10.Input.Text.Margin">
        <item name="android:layout_margin">@dimen/skillz_i10_text_field_margin</item>
    </style>
    <style name="Skillz.I10.Input.Text.ReadOnly">
        <item name="android:contentDescription">@string/skillz_font_helvetica_bold</item>
        <item name="android:textColor">@color/skillz_i10_text_gray</item>
    </style>
    <style name="Skillz.I10.Input.Text.ReadOnly.Margin">
        <item name="android:layout_margin">@dimen/skillz_i10_text_field_margin</item>
    </style>
    
    <style name="Skillz.I10.Input.CheckBox">
        <item name="android:button">@drawable/skillz_i10_btn_checkbox</item>
        <item name="android:background">@drawable/skillz_i10_bg_text_field</item>
        <item name="android:layout_height">40dip</item>
        <item name="android:layout_width">40dip</item>
    </style>
    
    <!-- 
      ~ This style must also be accompanied by a custom adapter to set
      ~ the textSize, typeface, and textColor 
      -->
